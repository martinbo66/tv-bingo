{
  "name": "vite-vue-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:web": "vue-tsc -b && vite build --base=./ --mode production",
    "preview": "vite preview --base=./",
    "preview:web": "npm run build:web && vite preview --base=./",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.5.10",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/ui": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.3.7",
    "jsdom": "^27.4.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vitest": "^4.0.18",
    "vue-tsc": "^2.1.6"
  }
}
